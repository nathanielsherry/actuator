#!/usr/local/bin/act

#This flowset should print "[['hello', 'world'], {'h2': 'hello', 'w2': 'world'}]" once

flow h from "hello" to @target;
flow w from "world" to @target;

flow target
  from inflows
  to @out
  on start;



flow h2 from "hello" to @named;
flow w2 from "world" to @named;

flow named
  from inflows(named)
  to @out;


flow out from inflows;
